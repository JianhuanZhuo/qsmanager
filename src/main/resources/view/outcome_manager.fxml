<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>

<?import javafx.scene.layout.VBox?>
<VBox fx:id="root" alignment="CENTER" styleClass="content-pane" stylesheets="@css/common.css"
      xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="cn.keepfight.qsmanager.controller.OutComeManagerController">
    <Pane VBox.vgrow="ALWAYS"/>
    <HBox>
        <Pane HBox.hgrow="ALWAYS"/>
        <Button fx:id="receiptBtn" contentDisplay="TOP" styleClass="btn-common" text="供应商送货列表">
            <graphic>
                <ImageView>
                    <Image url="@img/6.png"/>
                </ImageView>
            </graphic>
        </Button>
        <Pane HBox.hgrow="ALWAYS"/>
        <Button fx:id="annuBtn" contentDisplay="TOP" styleClass="btn-common" text="年度对账表">
            <graphic>
                <ImageView>
                    <Image url="@img/1.png"/>
                </ImageView>
            </graphic>
        </Button>
        <Pane visible="false" managed="false" HBox.hgrow="ALWAYS"/>
        <Button visible="false" managed="false" fx:id="staticBtn" contentDisplay="TOP" styleClass="btn-common"
                text="购付汇总">
            <graphic>
                <ImageView>
                    <Image url="@img/2.png"/>
                </ImageView>
            </graphic>
        </Button>
        <Pane HBox.hgrow="ALWAYS"/>
        <Button fx:id="predBtn" contentDisplay="TOP" styleClass="btn-common" text="付款预算表">
            <graphic>
                <ImageView>
                    <Image url="@img/3.png"/>
                </ImageView>
            </graphic>
        </Button>
        <Pane HBox.hgrow="ALWAYS"/>
        <Button fx:id="salaryBtn" contentDisplay="TOP" styleClass="btn-common" text="工资表">
            <graphic>
                <ImageView>
                    <Image url="@img/4.png"/>
                </ImageView>
            </graphic>
        </Button>
        <Pane HBox.hgrow="ALWAYS"/>
        <Button fx:id="rayBtn" contentDisplay="TOP" styleClass="btn-common" text="抵扣表">
            <graphic>
                <ImageView>
                    <Image url="@img/5.png"/>
                </ImageView>
            </graphic>
        </Button>
        <Pane HBox.hgrow="ALWAYS"/>
    </HBox>
    <Pane VBox.vgrow="ALWAYS"/>
</VBox>