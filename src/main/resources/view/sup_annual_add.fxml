<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.DatePicker?>
<VBox fx:id="root" styleClass="title-pane" stylesheets="@css/common.css" xmlns="http://javafx.com/javafx/8.0.111"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="cn.keepfight.qsmanager.controller.SupAnnualAddController">
    <HBox styleClass="title-line">
        <Label text="编辑对账明细信息"/>
        <Pane HBox.hgrow="ALWAYS"/>
    </HBox>
    <GridPane styleClass="infogp">
        <Label text="日期：" GridPane.columnIndex="0" GridPane.rowIndex="0"/>
        <Label text="金额：" GridPane.columnIndex="0" GridPane.rowIndex="1"/>
        <Label text="开票单位：" GridPane.columnIndex="0" GridPane.rowIndex="2"/>
        <Label text="开票日期：" GridPane.columnIndex="0" GridPane.rowIndex="3"/>
        <Label text="开票金额：" GridPane.columnIndex="0" GridPane.rowIndex="4"/>
        <Label text="税率：" GridPane.columnIndex="0" GridPane.rowIndex="5"/>
        <Label text="税金：" GridPane.columnIndex="0" GridPane.rowIndex="6"/>
        <Label text="汇款单位：" GridPane.columnIndex="0" GridPane.rowIndex="7"/>
        <Label text="汇款方式：" GridPane.columnIndex="0" GridPane.rowIndex="8"/>
        <Label text="汇款日期：" GridPane.columnIndex="0" GridPane.rowIndex="9"/>
        <Label text="本月付款金额：" GridPane.columnIndex="0" GridPane.rowIndex="10"/>
        <Label text="备注：" GridPane.columnIndex="0" GridPane.rowIndex="11"/>
        <TextField fx:id="mon" disable="true" GridPane.columnIndex="1" GridPane.rowIndex="0"/>
        <TextField fx:id="total" disable="true" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
        <TextField fx:id="billunit" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
        <DatePicker fx:id="billdate" GridPane.columnIndex="1" GridPane.rowIndex="3"/>
        <TextField fx:id="billtotal" GridPane.columnIndex="1" GridPane.rowIndex="4"/>
        <TextField fx:id="rate" GridPane.columnIndex="1" GridPane.rowIndex="5"/>
        <TextField fx:id="ratetotal" disable="true" GridPane.columnIndex="1" GridPane.rowIndex="6"/>
        <TextField fx:id="remitunit" GridPane.columnIndex="1" GridPane.rowIndex="7"/>
        <TextField fx:id="pattern" GridPane.columnIndex="1" GridPane.rowIndex="8"/>
        <DatePicker fx:id="remitdate" GridPane.columnIndex="1" GridPane.rowIndex="9"/>
        <TextField fx:id="paytotal" GridPane.columnIndex="1" GridPane.rowIndex="10"/>
        <TextArea fx:id="note" prefHeight="50.0" prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="11"/>
        <columnConstraints>
            <ColumnConstraints halignment="RIGHT"/>
            <ColumnConstraints/>
        </columnConstraints>
        <rowConstraints>
            <RowConstraints/>
            <RowConstraints/>
            <RowConstraints/>
            <RowConstraints/>
            <RowConstraints/>
            <RowConstraints/>
            <RowConstraints/>
            <RowConstraints/>
            <RowConstraints/>
            <RowConstraints/>
            <RowConstraints/>
            <RowConstraints/>
        </rowConstraints>
    </GridPane>
</VBox>
